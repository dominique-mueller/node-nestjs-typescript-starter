{
  "name": "node-nestjs-typescript-starter",
  "version": "1.0.0",
  "description": "Starter for NestJS projects using TypeScript",
  "license": "MIT",
  "scripts": {
    "build": "webpack --mode production",
    "start": "concurrently --names \"BUILD,APP\" \"webpack --mode development\" \"wait-on build/app.js && node build/app.js\"",
    "start:prod": "node build/app.js",
    "test": "jest --config jest.config.json",
    "test:watch": "jest --config jest.config.json --watch",
    "test:e2e": "jest --config jest.e2e.config.json",
    "test:e2e:watch": "jest --config jest.e2e.config.json --watch"
  },
  "dependencies": {
    "@nestjs/common": "6.5.x",
    "@nestjs/core": "6.5.x",
    "@nestjs/platform-express": "6.5.x",
    "reflect-metadata": "0.1.x",
    "rxjs": "6.5.x"
  },
  "devDependencies": {
    "@nestjs/testing": "6.5.x",
    "@types/express": "4.17.x",
    "@types/jest": "24.0.x",
    "@types/node": "12.6.x",
    "@types/supertest": "2.0.x",
    "@types/terser-webpack-plugin": "1.2.x",
    "@types/webpack-env": "1.14.x",
    "@types/webpack-node-externals": "1.6.x",
    "@types/webpack": "4.4.x",
    "clean-webpack-plugin": "3.0.x",
    "concurrently": "4.1.x",
    "jest": "24.8.x",
    "supertest": "4.0.x",
    "terser-webpack-plugin": "1.3.x",
    "ts-jest": "24.0.x",
    "ts-loader": "6.0.x",
    "ts-node": "8.3.x",
    "typescript": "3.5.x",
    "wait-on": "3.3.x",
    "webpack-cli": "3.3.x",
    "webpack-node-externals": "1.7.x",
    "webpack": "4.36.x"
  }
}
